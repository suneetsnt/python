def srecords(snumber,scode,sname):
    

    print("Student number")
    
    if len(str(snumber))==8:
        print (int(snumber))
        
    else:
        print("Incorrect Input")
        
    print("Subject code")
    if len(str(scode))==7:
        print (int(scode))
    else:
        print("Incorrect Input")
        
    print("Student name is")
    print(str(sname))
        
